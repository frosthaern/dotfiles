set nocompatible
filetype on
filetype plugin indent on
syntax on
set nu
set clipboard = "unnamedplus"
set rnu
set shiftwidth=4
set ruler
set encoding=utf-8
set wrap
set autoindent
set smartindent
set tabstop=2 shiftwidth=2 expandtab
set wildmenu
set wildmode=list:longest
set hidden
set ttyfast
colorscheme elflord
